{
    "name": "backend",
    "version": "1.0.0",
    "description": "StockStalker - Backend API",
    "main": "app.js",
    "repository": "github:Stock-Stalker/backend",
    "scripts": {
        "test": "mocha ./tests/*.js --exit",
        "coverage": "nyc npm run test && nyc report --reporter=text --check-coverage --lines 90",
        "start": "node app.js",
        "dev": "nodemon app.js",
        "format": "prettier --write \"{utils,tests,routes,models,middleware,controllers}/*.js\" && prettier --write ./app.js",
        "lint-files": "eslint --fix \"{utils,routes,models,middleware,controllers}/*.js\" && eslint --fix ./app.js",
        "lint": "npm run format && npm run lint-files"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.21.1",
        "bcrypt": "^5.0.1",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "express": "^4.17.1",
        "express-validator": "^6.10.0",
        "flatted": "^3.1.1",
        "helmet": "^4.6.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.12.5",
        "redis": "^3.0.2"
    },
    "devDependencies": {
        "chai": "^4.3.4",
        "chai-http": "^4.3.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.3.1",
        "mocha": "^8.3.2",
        "nodemon": "^2.0.7",
        "nyc": "^15.1.0",
        "prettier": "2.2.1",
        "snyk": "^1.530.0"
    }
